{
  "name": "create-olive-app",
  "version": "1.0.0",
  "description": "",
  "main": "tailwind.config.js",
  "scripts": {
    "setup": "npm run setup:windows || npm run setup:unix",
    "setup:windows": ". scripts/setup.sh",
    "setup:unix": "./scripts/setup.sh",
